{
    "name": "osw3/symfony-bundle-base",
    "description": "Add bundle-base to your app pages.",
    "homepage": "https://osw3.net",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
		{
			"name": "Project Contributors",
			"homepage": "https://github.com/osw3/symfony-bundle-base/graphs/contributors"
		}
    ],
    "autoload": {
        "psr-4": {
            "OSW3\\Base\\": "src/"
        }
    },
    "require": {
        "osw3/symfony-trait": "*",
        "symfony/twig-bundle": "6.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.0"
    }
}